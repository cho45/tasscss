=== empty
--->

=== nesting
a,
b {
	l1 : foo;

	c, d {
		l2 : foo;
		e, f {
			l3 : foo;
		}
	}
}
--->
a,
b {
l1 : foo;
}
a c,
a d,
b c,
b d {
l2 : foo;
}
a c e,
a c f,
a d e,
a d f,
b c e,
b c f,
b d e,
b d f {
l3 : foo;
}

=== mixin
@mixin foo {
	foo : bar;
}

a {
	@include foo;
}

b {
	@include foo;
}
--->
a {
foo : bar;
}
b {
foo : bar;
}

=== mixin
@mixin foo ($a, $b) {
	foo : $a $b;
}

a {
	@include foo(#000, #111);
	@include foo(#000, rgba(0, 0, 0, 0.5));
	@include foo(#000, "foo, bar \" baz");
	@include foo(#000, 'foo, bar \' baz');
}
--->
a {
foo : #000 #111;
foo : #000 rgba(0, 0, 0, 0.5);
foo : #000 "foo, bar \" baz";
foo : #000 'foo, bar \' baz';
}

=== mixin and include
@mixin foo {
	foo;
}
@mixin bar {
	@include foo;
	bar;
}

x {
	@include bar;
}
--->
x {
foo;
bar;
}

=== mixin and include 2
@mixin foo {
	foo;
}
@mixin bar {
	@include foo;
	bar;
}
@mixin baz {
	@include bar;
	baz;
}

x {
	@include baz;
}
--->
x {
foo;
bar;
baz;
}

=== variable
$foo : foo;
$bar : bar;

foo : $foo;
bar : $bar;

scope1 {
	$foo : foo1;
	foo : $foo;
	bar : $bar;
	scope2 {
		$foo : foo2;
		$bar : bar2;
		foo : $foo;
		bar : $bar;
	}
}

foo : $foo;
bar : $bar;
--->
foo : foo;
bar : bar;
scope1 {
foo : foo1;
bar : bar;
}
scope1 scope2 {
foo : foo2;
bar : bar2;
}
foo : foo;
bar : bar;

=== variables mixins continuation 1
$foo : foo;
@mixin mixin1 {
	bar : bar;
}
--->
=== variable mixins continuation 2
foo : $foo;
@include mixin1;
--->
foo : foo;
bar : bar;

=== variable contains 2 or more ":"
$a : "b : c";
a : $a;
--->
a : "b : c";
